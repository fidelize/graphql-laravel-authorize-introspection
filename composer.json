{
    "name": "fidelize/graphql-laravel-authorize-introspection",
    "description": "Authorize introspection documentarion for rebing/graphql-laravel",
    "keywords": ["framework", "laravel", "graphql", "graphql-laravel"],
    "authors": [
        {
            "name": "Fidelize IT",
            "homepage": "https://www.fidelize.com.br",
            "role": "Company"
        },
        {
            "name": "Alan Willms",
            "email": "alanwillms@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "rebing/graphql-laravel": ">=1.15.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5|~6.0|~7.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Fidelize\\GraphQLAuthorizedIntrospection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidelize\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidelize\\GraphQLAuthorizedIntrospection\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
